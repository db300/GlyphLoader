@page
@model WaterTrans.GlyphLoader.Demo.Pages.SVGRenderModel
@{
    Layout = null;
}
<svg width="@Model.Width" height="@Model.Height" viewBox="0 0 @Model.Width @Model.Height" xmlns='http://www.w3.org/2000/svg' version='1.1'>
    @foreach (var glyph in Model.Glyphs)
    {
        <path d="@glyph.GlyphOutline.Figures.ToString(glyph.X, glyph.Y)" fill="black" stroke="black" stroke-width="0" />
    }
</svg>